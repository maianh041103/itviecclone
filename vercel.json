{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PATCH",
        "PUT",
        "DELETE"
      ]
    }
  ],
  "functions": {
    "installAndBuild": {
      "handler": "vercel.json::npmInstallAndBuild"
    }
  }
}